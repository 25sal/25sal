	author = {Aversa, Rocco and Avvenuti, M and Cuomo, Antonio and DI MARTINO, B and DI MODICA, G and Distefano, S and Puliafito, A and Rak, M and Tomarchio, O and Vecchio, Alessio and Venticinque, S and Villano, U},
	title = {The Cloud@Home Project: Towards a New Enhanced Computing Paradig},
	year = {2011},
	publisher = {Springer},
